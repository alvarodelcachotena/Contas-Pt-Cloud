# External Document Processing API Keys
# Add your API keys for external document processing services
# Each service offers different capabilities and pricing models

# VisionParser.com - High-accuracy receipt/invoice processing
# Free trial: 30 days, then usage-based pricing (~$0.01/document)
# Best for: Receipts, invoices, financial documents
VISIONPARSER_API_KEY=your_visionparser_api_key_here

# Mindee Invoice OCR - Specialized invoice processing
# Free tier: 250 pages/month, then $0.01-$0.10/page
# Best for: Multi-page invoices, 50+ languages
MINDEE_API_KEY=your_mindee_api_key_here

# Klippa OCR - Financial document specialist
# Contact for pricing, competitive rates
# Best for: Complex tables, financial documents
KLIPPA_API_KEY=your_klippa_api_key_here

# Microsoft Azure Form Recognizer - Enterprise-grade
# Pay-per-use model, enterprise features
# Best for: Forms, invoices, enterprise integration
AZURE_FORM_RECOGNIZER_KEY=your_azure_key_here
AZURE_FORM_RECOGNIZER_ENDPOINT=https://your-resource.cognitiveservices.azure.com/

# Google Document AI - Scalable processing
# Pay-per-document processed
# Best for: Various documents, custom models
GOOGLE_DOCUMENT_AI_KEY=your_google_ai_key_here
GOOGLE_DOCUMENT_AI_PROJECT_ID=your_project_id_here

# Veryfi API - Real-time processing
# SOC2 Type2 & FedRAMP compliant
# Best for: Real-time apps, mobile receipts
VERYFI_CLIENT_ID=your_veryfi_client_id_here
VERYFI_CLIENT_SECRET=your_veryfi_client_secret_here
VERYFI_USERNAME=your_veryfi_username_here

# Amazon Textract - AWS Integration
# Pay-per-page processed
# Best for: AWS infrastructure, text/tables
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here
AWS_REGION=us-east-1

# Processing Strategy Configuration
DEFAULT_PROCESSOR=auto  # auto, visionparser, mindee, klippa, azure, google, veryfi
CONFIDENCE_THRESHOLD=0.75
ENABLE_TABLE_EXTRACTION=true
ENABLE_VISION_PARSING=true
MAX_RETRIES=3

# Cost Management
MAX_COST_PER_DOCUMENT=0.10  # Maximum cost per document (USD)
PREFERRED_COST_RANGE=0.05   # Preferred maximum cost
ENABLE_COST_OPTIMIZATION=true